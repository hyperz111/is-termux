{
	"name": "is-termux",
	"version": "1.0.0",
	"description": "Check if you run in Termux environment",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"scripts": {
		"test": "pnpm run build && node --test",
		"build": "tsc",
		"publint": "publint",
		"format": "prettier -w **/*.{js*,ts} !dist"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"check",
		"checking",
		"termux",
		"android",
		"linux",
		"terminal",
		"emulator"
	],
	"author": "Hyper-Z11 (https://github.com/hyperz111)",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hyperz111/is-termux.git"
	},
	"bugs": {
		"url": "https://github.com/hyperz111/is-termux/issues"
	},
	"homepage": "https://github.com/hyperz111/is-termux#readme",
	"packageManager": "pnpm@10.13.1",
	"devDependencies": {
		"@hyperz111/prettier-config": "^1.0.0",
		"@types/node": "^24.3.0",
		"prettier": "^3.6.2",
		"publint": "^0.3.12",
		"typescript": "^5.9.2"
	}
}
